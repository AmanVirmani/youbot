<!--***************************************** MANIPULATOR ******************************************-->


	<joint name="chassis__base_link" type="fixed">
		<origin xyz="0 0 0.07" rpy="0 0 0"/>
		<axis xyz="0 0 1"/>
    <parent link="chassis"/>
    <child link="base_link"/>
		<limit effort="0" velocity="0" lower="3.14" upper="-3.14"/>
	</joint>

  <link name="base_link">
		<inertial>
			<origin xyz="0 0 0.05" rpy="0 0 0"/>
			<mass value="1.0"/>
			<inertia ixx="0.1458" iyy="0.1458" izz="0.1250" ixy="0" ixz="0" iyz="0"/>
		</inertial>
		<collision>
			<origin xyz="0 0 0.05" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.09" length="0.1"/>
			</geometry>
		</collision>
		<visual>
			<origin xyz="0 0 0.05" rpy="0 0 0"/>
			<geometry> 
				<cylinder radius="0.09" length="0.1"/>
			</geometry>
			<material name="green">
				<color rgba="0 1 0 0.7"/>
			</material>
		</visual>
	</link>

